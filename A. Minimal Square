#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--)
    {
        int a, b;
        cin>>a>>b;
        int x, y;
        x = max(a, 2 * b);
        y = max(b, 2 * a);
        int ans = min(x, y);
        ans = ans * ans;
        cout<<ans<<endl;

    }
    
    
    return 0;
}
