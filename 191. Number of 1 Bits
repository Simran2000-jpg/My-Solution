class Solution {
public:
    int hammingWeight(uint32_t n) {
      //  string str="";
            string str = bitset<32>(n).to_string();
            int cnt=0;int N=str.size();
            cout<<str<<endl;
            for(int i=0;i<N;i++){
                    if(str[i]=='1')cnt++;
            }
            return cnt;
    }
};
